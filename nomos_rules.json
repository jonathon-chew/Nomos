{
	"functions-have-doc-strings": true,
	"variable-names": "pascal_case",
	"function-names": "kebab_case",
	"print-f-new-line": true,
	"only-show-errors": true,
	"ignore-if-in-comments": true,
	"exported-identifiers-have-comments": true,
	"const-in-caps": true,
	"no-naked-returns": false,
	"readme-stats": true,
	"show-stat-rules": false,
	"ignore-main-function": false
}
